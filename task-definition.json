{
  "family": "hello-world-aws-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::798004768642:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hello-world-aws-container",
      "image": "798004768642.dkr.ecr.us-east-1.amazonaws.com/hello-world-aws:latest",
      "portMappings": [
        {
          "containerPort": 7101,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/hello-world-aws-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}